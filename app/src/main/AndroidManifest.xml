<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.infinity">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_inf"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_inf"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.About"></activity>
        <activity android:name=".activities.Profile" />
        <activity android:name=".activities.TestAttempts" />
        <activity android:name=".activities.TestAnalysis" />
        <activity android:name=".activities.StudentTest" />
        <activity android:name=".activities.EditQuestion" />
        <activity android:name=".activities.TeacherTest" />
        <activity
            android:name=".activities.StudentDashboard"
            android:label="Dashboard" />
        <activity
            android:name=".activities.TeachersDashboard"
            android:label="Dashboard" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>